<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<?import java.net.URL?>

<BorderPane maxHeight="300.0" maxWidth="396.0" minHeight="300.0" minWidth="396.0" prefHeight="300.0" prefWidth="396.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="de.jannks.controllers.StartController">
   <stylesheets>
      <URL value="@../styles/start.css" />
   </stylesheets>
   <top>
      <HBox prefHeight="26.0" prefWidth="396.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="button64" mnemonicParsing="false" text="64-bit" />
            <Button fx:id="button128" mnemonicParsing="false" text="128-bit" />
            <Button fx:id="button256" mnemonicParsing="false" text="256-bit" />
            <Button fx:id="button512" mnemonicParsing="false" text="512-bit" />
            <Button fx:id="button1024" mnemonicParsing="false" text="1024-bit" />
            <Button fx:id="button2048" mnemonicParsing="false" text="2048-bit" />
            <Button fx:id="button4096" mnemonicParsing="false" text="4096-bit" />
         </children>
      </HBox>
   </top>
   <center>
      <TextArea fx:id="textKey" prefHeight="274.0" prefWidth="396.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
